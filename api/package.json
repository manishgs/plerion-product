{
  "name": "plerion-product-api",
  "description": "This template demonstrates how to make a simple HTTP API with Node.js and Typescript running on AWS Lambda and API Gateway using the Serverless Framework v3",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "serverless offline start --reloadHandler",
    "deploy": "serverless deploy",
    "invoke": "serverless invoke local --function",
    "lint": "eslint \"**/*.{js,ts}\" --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{ts,md,json,yml}\""
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "serverless": "^3.21.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "serverless-tscpaths": "^0.0.8",
    "typescript": "*"
  },
  "dependencies": {
    "aws-sdk": "^2.1381.0",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  }
}
